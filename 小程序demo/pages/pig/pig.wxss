/* pages/pig/pig.wxss */
.main{
  width: 100%;
  height: 150px;
  background-color: red;
  text-align: center;
  color: #fff;
  margin-bottom: 20px;
}
.main image{
  margin: 20px 0 10px 0;
  width: 60px;
  height: 60px;
  border-radius: 50px;
}
.list{
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
}
.list navigator{
  /* margin-top: 10px; */
  height: 44px;
  border-bottom:1px solid #eee;
  background-color: #fff;
  justify-content: space-between;
  align-items: center;
  display: flex;
  margin-left: 10px;
  padding-right: 10px;
  color: #999;
}
.list navigator view{
  height: 44px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.list navigator image{
  margin: auto 10px auto 0;
}
.list image{
  height: 24px;
  width: 24px;
}